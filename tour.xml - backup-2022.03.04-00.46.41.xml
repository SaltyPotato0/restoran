	<scene name="scene_Ulaz_spolja" title="Ulaz spolja" onstart="" thumburl="panos/Ulaz_spolja.tiles/thumb.jpg" lat="" lng="" heading="">
		
		<control bouncinglimits="calc:image.cube ? true : false" />

